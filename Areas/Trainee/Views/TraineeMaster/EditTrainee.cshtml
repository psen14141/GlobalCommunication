                                                document.getElementById("TraineeNo").value = nextTraingNumber;
                                                //alert(nextTraingNumber);
                                            });

                                        }
                                        function clearImage() {
                                            $('#userImg')
                                                .attr('src', "/assets/ImagesS/User.png")
                                                .width(91)
                                                .height(97);
                                            $('input[type=file]').val(null);
                                            var remove = document.getElementById("TraineeImage");
                                            remove.removeChild(remove.childNodes[0]);
                                            //var inputfile = $('#TraineeImage');
                                            //inputfile.replaceWith(inputfile.val('').clone(true));

                                        }

                                        function GetImage() {
                                            $("#TraineeImage").click();
                                        }
                                        function readURL(input) {
                                            if (input.files && input.files[0]) {
                                                var reader = new FileReader();

                                                reader.onload = function (e) {
                                                    $('#userImg')
                                                        .attr('src', e.target.result)
                                                        .width(91)
                                                        .height(97);
                                                };

                                                reader.readAsDataURL(input.files[0]);
                                            }
                                        }


                                        function showTooltip() {
                                            $('#tooltipBtn').tooltip({
                                                trigger: 'click',
                                                placement: 'top'

                                            });


                                            var clipboard = new Clipboard('#tooltipBtn');

                                            var imgSrc = document.getElementById('userImg').getAttribute('src');

                                            var myArr = imgSrc.split("=");

                                            var imgPath = "";

                                            if (myArr[1] === "") {
                                                var imgObj = document.getElementById('TraineeImage').files[0];
                                                if (imgObj == undefined) {
                                                    alert("画像パスがありません。");
                                                    return;
                                                }
                                                var fileName = imgObj.name;
                                                imgPath = "/DataFolder/" + fileName;
                                            } else {

                                                imgPath = myArr[1];

                                            }

                                            if (imgPath.length > 0) {

                                                clipboard.on('success', function (e) {
                                                    var btn = $(e.trigger);
                                                    setTooltip(btn, imgPath);
                                                    hideTooltip(btn);
                                                    var remove = document.getElementById("TraineeImage");
                                                    remove.removeChild(remove.childNodes[0]);
                                                });
                                            } else {
                                                alert("画像パスがありません。");
                                            }



                                        }
                                        function setTooltip(btn, message) {
                                            btn.tooltip('hide')
                                                .attr('data-original-title', message)
                                                .tooltip('show');
                                        }

                                        function hideTooltip(btn) {
                                            setTimeout(function () {
                                                btn.tooltip('hide');
                                            }, 1000);
                                        }
                                    </script>
