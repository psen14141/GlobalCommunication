@model GloableCommunication.Areas.Trainee.Models.ViewModel.TraineeMasterDetailView

@{
    ViewData["Title"] = "DetailViewTrainee";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>

<script>
    $('#bologna-list a').on('click', function (e) {
        e.preventDefault()
        $(this).tab('show')
    })
</script>

<style>
    body {
        font-family: Arial, Helvetica, sans-serif;
    }

    .modal {
        position: center;
        width: 70%;
        height: 70%;
        background-color: rgb(0,0,0);
        background-color: rgba(0,0,0,0.9);
    }

    .modal-content {
        margin: auto;
        display: block;
        width: 50%;
        max-width: 400px;
    }

    #caption {
        margin: auto;
        display: block;
        width: 50%;
        max-width: 500px;
        text-align: center;
        color: #ccc;
        padding: 10px 0;
        height: 50px;
    }

    .close {
        position: absolute;
        top: 5px;
        right: 15px;
        color: #f1f1f1;
        font-size: 40px;
        font-weight: bold;
        transition: 0.3s;
    }

        .close:hover,
        .close:focus {
            color: #bbb;
            text-decoration: none;
            cursor: pointer;
        }
</style>

<style>
    .bs-example {
        margin: 20px;
    }
</style>

<style>
    #parent {
        display: flex;
    }

    #narrow {
        width: 500px;
    }

    #wide {
        flex: 1;
    }

    canvas {
        width: 800px !important;
        height: 400px !important;
    }

    #outer {
        width: 100%;
        text-align: center;
    }

    .inner {
        display: inline-block;
    }

    body {
        padding: 2rem 0rem;
    }
</style>

<div>
    <button class="w3-button w3-green" onclick="goBack()"><i class="fa fa-arrow-left"></i></button>
    @*<button class="w3-button" style="background-color: #e7e7e7; color:blue;" onclick="ClearData()"><i class="fa fa-undo"></i></button>*@
    <button class="w3-button w3-blue"><i class="fa fa-print"></i></button>
</div>

<div class="form-group">
    <div class="row">
        <a asp-action="Create" style="font-size:14px; padding-left:10px">戻 る</a>
        @*<a asp-action="Create" style="font-size:14px; padding-left:10px">編 集</a>*@
        <a asp-action="Create" style="font-size:14px; padding-left:15px"> 印 刷</a>
        <div class="col-md-2 align-self-center" style="padding-left:630px;"><label asp-for="UpdateDate" class="col-md-12 text-right"> 更新日:</label></div>
        <div class="col-md-3 align-self-center" style="padding-left:50px;">
            <span>@Model.TraineeMaster.UpdateDate</span>
        </div>
    </div>
</div>

<div class=" w3-margin-top w3-gray">
    <h5 style="text-align:center">実習生情報</h5>
</div>

<div id="parent">
    <div id="wide">
        <input name="TraineeId" id="TraineeId" type="hidden" class="form-control" value="@Model.TraineeMaster.TraineeId" />
        <div class="card card-custom mx-2 mb-3">
            <div class="card-body">

                <div class="row w3-margin-top w3-gray">
                    <div class="col col-md-12">
                        <h5 style="text-align:center">パスポート・外国人登録情報</h5>
                    </div>
                </div>

                <div class="row form-group" style="padding-top:10px;">
                    <div class="col">
                        <div class="row">

                            <div class="col col-md-2"><label asp-for="PassportNo">パスポートNo</label></div>
                            <div class="col col-md-4">
                                <input name="PassportNo" id="PassportNo"
                                       value="@foreach (var row in Model.traineeHistories)
                                    {

                                        @row.PassportNo

                                    }" class="form-control" disabled />
                                <span asp-validation-for="PassportNo" class="text-danger"></span>
                            </div>

                            <div class="col col-md-2"><label asp-for="PassportLimit">有効期限</label></div>
                            <div class="col col-md-4">
                                <input name="PassportLimit" id="PassportLimit"
                                       value=" @foreach (var row in Model.traineeHistories)
                                    {

                                        @row.PassportLimit

                                    }" class="form-control" disabled />
                                <span asp-validation-for="PassportLimit" class="text-danger"></span>
                            </div>

                        </div>
                    </div>
                </div>

                <div class="row form-group" style="padding-top:2px;">
                    <div class="col">
                        <div class="row">

                            <div class="col col-md-2"><label asp-for="ResidentCardNo">在留カード番号</label></div>
                            <div class="col col-md-4">
                                <input name="ResidentCardNo" id="ResidentCardNo"
                                       value="@foreach (var row in Model.traineeHistories)
                                    {

                                        @row.ResidentCardNo

                                    }" class="form-control" disabled />
                                <span asp-validation-for="ResidentCardNo" class="text-danger"></span>
                            </div>

                            <div class="col col-md-2"><label asp-for="ResidentCardExpireDate">在留期限満了日</label></div>
                            <div class="col col-md-4">
                                <input name="ResidentCardExpireDate" id="ResidentCardExpireDate"
                                       value="@foreach (var row in Model.traineeHistories)
                                    {

                                        @row.ResidentCardExpireDate

                                    }" class="form-control" disabled />

                                <span asp-validation-for="ResidentCardExpireDate" class="text-danger"></span>
                            </div>

                        </div>
                    </div>
                </div>

            </div>
        </div>

        @***********************Trainee Information Start******************************@

        <div class="accordion" id="accordionExample">
            <div class="card" style="margin:1px;">
                <div class="card-header" id="headingOne">
                    <h2 class="mb-0">
                        <button type="button" class="btn btn-link" data-toggle="collapse" data-target="#collapseOne">実習生情報</button>
                    </h2>
                </div>
                <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordionExample">
                    <div class="card-body">

                        <div class="row form-group" style="padding-top:1px;">
                            <div class="col">
                                <div class="row">
                                    <div class="col col-md-3"><label asp-for="TraineeNo">実習生No</label></div>
                                    <div class="col col-md-3">
                                        <input name="TraineeNo" id="TraineeNo" value="@Model.TraineeMaster.TraineeNo" class="form-control" disabled />
                                        <span asp-validation-for="TraineeNo" class="text-danger"></span>
                                    </div>
                                    <div class="col col-md-2"><label asp-for="Certification1No">認定番号</label></div>
                                    <div class="col col-md-3">
                                        <input name="Certification1No" id="Certification1No" value="@Model.TraineeMaster.Certification1No" class="form-control" disabled />
                                        <span asp-validation-for="Certification1No" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row form-group" style="padding-top:1px;">
                            <div class="col">
                                <div class="row">
                                    <div class="col col-md-3"><label asp-for="Schlarship">期生</label></div>
                                    <div class="col col-md-2">
                                        <input name="Schlarship" id="Schlarship" class="form-control" value="@Model.TraineeMaster.Schlarship" disabled />
                                        <span asp-validation-for="Schlarship" class="text-danger"></span>
                                    </div>
                                    <div class="col col-md-1"><label asp-for="Schlarship">期生</label></div>

                                    <div class="col col-md-2"><label asp-for="DelayedEntry">入国遅れ</label></div>
                                    <div class="col col-md-3">
                                        <input name="DelayedEntry" id="DelayedEntry" class="form-control" value="@Model.TraineeMaster.DelayedEntry" disabled />
                                    </div>
                                </div>
                            </div>
                        </div>


                        <div class="row form-group" style="padding-top:1px;">
                            <div class="col">
                                <div class="row">
                                    <div class="col col-md-3"><label asp-for="NewOldSystem">新/旧制度</label></div>
                                    <div class="col col-md-5">
                                        <input name="NewOldSystem" id="NewOldSystem" class="form-control" value="@Model.TraineeMaster.NewOldSystem" disabled />
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row form-group" style="padding-top:2px;">
                            <div class="col col-md-9">
                                <div class="row">
                                    <div class="col col-md-4"><label asp-for="TraineeName">氏名(英)</label></div>
                                    <div class="col col-md-6">
                                        <input name="TraineeName" id="TraineeName" class="form-control" value="@Model.TraineeMaster.TraineeName" style="width:240px;" disabled />
                                        <span asp-validation-for="TraineeName" class="text-danger"></span>
                                    </div>
                                </div>

                                <div class="row" style="padding-top:10px;">
                                    <div class="col col-md-4"><label asp-for="KatakanaName">読み（カナ)</label></div>
                                    <div class="col col-md-6">
                                        <input name="KatakanaName" id="KatakanaName" class="form-control" value="@Model.TraineeMaster.KatakanaName" style="width:240px;" disabled />
                                        <span asp-validation-for="KatakanaName" class="text-danger"></span>
                                    </div>
                                </div>

                                <div class="row" style="padding-top:10px;">
                                    <div class="col col-md-4"><label asp-for="KanjiName">漢字表記</label></div>
                                    <div class="col col-md-6">
                                        <input name="KanjiName" id="KanjiName" class="form-control" value="@Model.TraineeMaster.KanjiName" style="width:240px;" disabled />
                                        <span asp-validation-for="KanjiName" class="text-danger"></span>
                                    </div>
                                </div>

                                <div class="row" style="padding-top:10px;">
                                    <div class="col col-md-4"><label asp-for="BloodTypeFlg">血液型</label></div>
                                    <div class="col col-md-6">
                                        <input name="BloodTypeFlg" id="BloodTypeFlg" class="form-control" value="@Model.TraineeMaster.BloodTypeFlg" style="width:240px;" disabled />
                                        <span asp-validation-for="BloodTypeFlg" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>

                            <div class="col col-md-3 border">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="thumbnail adventure-link" style="padding:25px 30px 30px 10px;">
                                            @*<a href="/Trainee/TraineeMaster/TraineeImage/?imagPath=@Model.TraineeMaster.TraineeImage" target="_blank">*@
                                            <img src="/Trainee/TraineeMaster/TraineeImage/?TraineeImage=@Model.TraineeMaster.TraineeImage" id="TraineeImage" alt="img" width="120" height="130" />
                                            @*</a>*@
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>

                        <div class="row form-group" style="padding-top:1px;">
                            <div class="col">
                                <div class="row">
                                    <div class="col col-md-3"><label asp-for="SexFlag">性別</label></div>
                                    <div class="col col-md-5 input-group-append">
                                        <input name="SexFlag" id="SexFlag" class="form-control" value="@Model.TraineeMaster.SexFlag" disabled />
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row form-group" style="padding-top:1px;">
                            <div class="col">
                                <div class="row">
                                    <div class="col col-md-3"><label asp-for="SpouseFlg">配偶者</label></div>
                                    <div class="col col-md-5 input-group-append">
                                        <input name="SpouseFlg" id="SpouseFlg" class="form-control" value="@Model.TraineeMaster.SpouseFlg" disabled />
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row form-group" style="padding-top:1px;">
                            <div class="col">
                                <div class="row">
                                    <div class="col col-md-3"><label asp-for="BirthDate">生年月日</label></div>
                                    <div class="col col-md-5 input-group-append">
                                        <input type="text" class="form-control" id="BirthDate" name="BirthDate" value="@Model.TraineeMaster.BirthDate" disabled />
                                    </div>
                                    <div class="col col-md-1 "><label asp-for="Age" class="ctext-right">歳</label></div>
                                    <div class="col col-md-2 ">
                                        <input name="Age" id="Age" class="form-control" value="@Model.TraineeMaster.Age" size="10" disabled />
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row form-group" style="padding-top:1px;">
                            <div class="col">
                                <div class="row">
                                    <div class="col col-md-3"><label asp-for="CountryId">国籍</label></div>
                                    <div class="col col-md-2">
                                        <input name="CountryId" id="CountryId" class="form-control" value="@Model.TraineeMaster.CountryId" size="10" disabled />
                                        <span asp-validation-for="CountryId" class="text-danger"></span>
                                    </div>
                                    <div class="col col-md-3">
                                        <input name="CountryName" id="CountryName" class="form-control"
                                               value=" @foreach (var row in Model.countries)
                                            {

                                                @row.CountryName

                                            }" size="10" disabled />
                                        <span asp-validation-for="CountryName" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row form-group" style="padding-top:1px;">
                            <div class="col">
                                <div class="row">
                                    <div class="col col-md-3"><label asp-for="BirthPlace">出生地</label></div>
                                    <div class="col col-md-5">
                                        <input name="BirthPlace" id="BirthPlace" class="form-control" value="@Model.TraineeMaster.BirthPlace" disabled />
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row form-group" style="padding-top:1px;">
                            <div class="col">
                                <div class="row">
                                    <div class="col col-md-3"><label asp-for="TraineeName">現住所 〒</label></div>
                                    <div class="col col-md-9">

                                        <div class="row">
                                            <div class="col col-md-1"><label asp-for="Address">〒</label></div>
                                            <div class="col col-md-3">
                                                <input name="Zip" id="Zip" class="form-control" value="@Model.TraineeMaster.Zip" disabled />
                                                <span asp-validation-for="Zip" class="text-danger"></span>
                                            </div>
                                            <div class="col col-md-8">
                                                <input name="Address1" id="Address1" class="form-control" value="@Model.TraineeMaster.Address1" disabled />
                                                <span asp-validation-for="Address1" class="text-danger"></span>
                                            </div>
                                        </div>

                                        <div class="row" style="padding-top:10px;">
                                            <div class="col col-md-12">
                                                <input name="Address2" id="Address2" class="form-control" value="@Model.TraineeMaster.Address2" disabled />
                                                <span asp-validation-for="Address2" class="text-danger"></span>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row form-group">
                            <div class="col">
                                <div class="row">
                                    <div class="col col-md-3"><label asp-for="TelephoneNo">連絡先 (Tel)</label></div>
                                    <div class="col col-md-4">
                                        <input name="TelephoneNo" id="TelephoneNo" class="form-control" value="@Model.TraineeMaster.TelephoneNo" disabled />
                                        <span asp-validation-for="TelephoneNo" class="text-danger"></span>
                                    </div>
                                    <div class="col col-md-1"><label asp-for="Email">Email</label></div>
                                    <div class="col col-md-4">
                                        <input name="Email" id="Email" class="form-control" value="@Model.TraineeMaster.Email" disabled />
                                        <span asp-validation-for="Email" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

            @*********************Trainee information End*************************@

            @*********************Training Period Information Start*************************@

            <div class="card" style="margin: 5px;">
                <div class="card-header" id="headingTwo">
                    <h2 class="mb-0">
                        <button type="button" class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapseTwo">実習期間情報</button>
                    </h2>
                </div>
                <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionExample">
                    <div class="card-body">

                        <div style="padding-top:10px;">
                            <div class="row">
                                <div class="col col-md-3"><label asp-for="Group" class="text-right">集合講習</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="GroupFrom" name="GroupFrom" value="@Model.TraineeMaster.GroupFrom" disabled />
                                </div>
                                <div class="col col-md-1"><label asp-for="Group" class="text-right">~</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="GroupTo" name="GroupTo" value="@Model.TraineeMaster.GroupTo" disabled />
                                </div>
                            </div>
                        </div>


                        <div style="padding-top:10px;">
                            <div class="row">
                                <div class="col col-md-3"><label asp-for="PracticalTraining" class="text-right">実習/ 1号</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="PracticalTrainingFrom" name="PracticalTrainingFrom" value="@Model.TraineeMaster.PracticalTrainingFrom" disabled />
                                </div>
                                <div class="col col-md-1"><label asp-for="PracticalTraining" class="text-right">~</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="PracticalTrainingTo" name="PracticalTrainingTo" value="@Model.TraineeMaster.PracticalTrainingTo" disabled />
                                </div>
                            </div>
                        </div>

                        <div style="padding-top:10px;">
                            <div class="row">
                                <div class="col col-md-3"><label asp-for="PracticalTraining2" class="text-right"> 実習/ 2号</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="PracticalTrainingFrom2" name="PracticalTrainingFrom2" value="@Model.TraineeMaster.PracticalTrainingFrom2" disabled />
                                </div>
                                <div class="col col-md-1"><label asp-for="PracticalTraining2" class="text-right">~</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="PracticalTrainingTo2" name="PracticalTrainingTo2" value="@Model.TraineeMaster.PracticalTrainingTo2" disabled />
                                </div>
                            </div>
                        </div>

                        <div style="padding-top:10px;">
                            <div class="row">
                                <div class="col col-md-3"><label asp-for="PracticalTraining3" class="ctext-right"> 実習/ 3号</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="PracticalTrainingFrom3" name="PracticalTrainingFrom3" value="@Model.TraineeMaster.PracticalTrainingFrom3" disabled />
                                </div>
                                <div class="col col-md-1"><label asp-for="PracticalTraining3" class="ctext-right">~</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="PracticalTrainingTo3" name="PracticalTrainingTo3" value="@Model.TraineeMaster.PracticalTrainingTo3" disabled />
                                </div>
                            </div>
                        </div>

                        <div style="padding-top:10px;">
                            <div class="row">
                                <div class="col col-md-3"><label asp-for="Technical" class="ctext-right">特定活動2年</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="TechnicalFrom" name="TechnicalFrom" value="@Model.TraineeMaster.TechnicalFrom" disabled />
                                </div>
                                <div class="col col-md-1"><label asp-for="Technical" class="ctext-right">~</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="TechnicalTo" name="TechnicalTo" value="@Model.TraineeMaster.TechnicalTo" disabled />
                                </div>
                            </div>
                        </div>

                        <div style="padding-top:10px;">
                            <div class="row">
                                <div class="col col-md-3"><label asp-for="SpecificActivity" class="ctext-right">特定活動3年</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="SpecificActivityFrom" name="SpecificActivityFrom" value="@Model.TraineeMaster.SpecificActivityFrom" disabled />
                                </div>
                                <div class="col col-md-1"><label asp-for="SpecificActivity" class="ctext-right">~</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="SpecificActivityto" name="SpecificActivityto" value="@Model.TraineeMaster.SpecificActivityto" disabled />
                                </div>
                            </div>
                        </div>

                        <div style="padding-top:10px;">
                            <div class="row">
                                <div class="col col-md-3"><label asp-for="SpecificNo1" class="ctext-right">特定技能</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="SpecificNo1From" name="SpecificNo1From" value="@Model.TraineeMaster.SpecificNo1From" disabled />
                                </div>
                                <div class="col col-md-1"><label asp-for="SpecificNo1" class="ctext-right">~</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="SpecificNo1To" name="SpecificNo1To" value="@Model.TraineeMaster.SpecificNo1To" disabled />
                                </div>
                            </div>
                        </div>

                        <div style="padding-top:10px;">
                            <div class="row">
                                <div class="col col-md-3"><label asp-for="SpecificNo2" class="ctext-right">特定技能 2</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="SpecificNo2From" name="SpecificNo2From" value="@Model.TraineeMaster.SpecificNo2From" disabled />
                                </div>
                                <div class="col col-md-1"><label asp-for="SpecificNo2" class="ctext-right">~</label></div>
                                <div class="col col-md-4">
                                    <input type="text" class="form-control" id="SpecificNo2To" name="SpecificNo2To" value="@Model.TraineeMaster.SpecificNo2To" disabled />
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            @*********************Training Period Information End*************************@

            @*********************Scanned Image Information Start*************************@
            <div class="card" style="margin: 1px;">
                <div class="card-header" id="headingFour">
                    <h2 class="mb-0">
                        <button type="button" class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapseFour">スキャン画像</button>
                    </h2>
                </div>
                <div id="collapseFour" class="collapse" aria-labelledby="headingFour" data-parent="#accordionExample">

                    <div style="padding-top:5px; ">
